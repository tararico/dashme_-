<p id="notice"><%= notice %></p>

<h1>Dashmebuttons</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Shop</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dashmebuttons.each do |dashmebutton| %>
      <tr>
        <td><%= dashmebutton.name %></td>
        <td><%= dashmebutton.shop %></td>
        <td><%= link_to 'Show', dashmebutton %></td>
        <td><%= link_to 'Edit', edit_dashmebutton_path(dashmebutton) %></td>
        <td><%= link_to 'Destroy', dashmebutton, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dashmebutton', new_dashmebutton_path %>
